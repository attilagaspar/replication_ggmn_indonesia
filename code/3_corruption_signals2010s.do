replace district="Kota Gunungsitoli" if district=="Kota Gunung Sitoli"
replace district="Kota Pare pare" if district=="Kota Pare-Pare"

tempfile k14
save `k14'

import excel "${raw_data_dir}/bpk reports/2015-2018.xlsx", sheet("Sheet1") clear


rename D district
rename F opinion2014_alt 
rename H opinion2015 
rename J opinion2016
rename L opinion2017
rename N opinion2018



drop if _n==1
drop if district==""
drop if strpos(district,"Prov.")!=0

keep district opinion*


replace district = subinstr(district,"Kab.","Kab. ",1) if substr(district,1,5)!="Kab. "
replace district = "Kab. Mamuju Utara" if district=="Kab. Pasangkayu (Mamuju Utara)"

merge 1:1 district using `k09', gen(m09)

merge 1:1 district using `k14', gen(m14)


drop if m14==2


order district opinion*, alpha


foreach v of varlist opinion2005 - opinion2018 {

	replace `v'=subinstr(`v',"*","",.)

}



*save "${processed_data_dir}/bpk_reports.dta", replace

foreach v of varlist opinion* {
	replace `v'=strtrim(`v')
	replace `v'="3" if `v'=="TMP"
	replace `v'="2" if `v'=="TW"
	replace `v'="1" if `v'=="WDP"
	replace `v'="0" if `v'=="WTP"
	replace `v'="0.5" if `v'=="WTP-DPP"|`v'=="WTP DPP"
	tab `v'
}
destring opinion*, force replace

drop opinion2014
rename opinion2014_alt opinion2014 // newer data point

rename opinion* opinion_numeric*

